{
	"name": "swift",
	"displayName": "Swift",
	"description": "Swift programming language support and supplemental tooling integration.",
	"version": "0.0.1",
	"preview": true,
	"license": "MPL-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/partiallyapplied/vscode-swift"
	},
	"bugs": {
		"url": "https://github.com/partiallyapplied/vscode-swift/issues"
	},
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Programming Languages",
		"Linters",
		"Formatters",
		"Other"
	],
	"keywords": [
		"swift",
		"swift-lang"
	],
	"activationEvents": [
		"onLanguage:swift",
		"workspaceContains:Package.swift"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"vscode-languageclient": "^6.1.3"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.3",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"prettier": "^2.0.5",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 4,
		"trailingComma": "es5"
	}
}
